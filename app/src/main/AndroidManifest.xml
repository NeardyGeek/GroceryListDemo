<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nerdygeek.grocerylistdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.GroceryListDemo">
        <activity android:name=".activities.SearchActivity"></activity>
        <activity android:name=".activities.EditAddressActivity" />
        <activity android:name=".activities.OrderDetailsActivity" />
        <activity android:name=".activities.OrdersActivity" />
        <activity android:name=".activities.AccountActivity" />
        <activity android:name=".activities.PaymentActivity" />
        <activity android:name=".activities.AddAddressActivity" />
        <activity android:name=".activities.AddressActivity" />
        <activity android:name=".activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.CartActivity" />
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.RegisterActivity" />
        <activity android:name=".activities.ProductActivity" />
        <activity android:name=".activities.SubCategoryActivity" />
        <activity android:name=".activities.MainActivity" />
    </application>

</manifest>